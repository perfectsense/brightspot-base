{
    "_template": "/assets/base/SocialFeed",

    "title": "{{words}}",

    "items": [
        {
            "_template": "/assets/base/SocialFeedItem",

            "_repeat": 3,

            "feedType": "facebook",

            "profilePhoto": {
                "_template": "/assets/base/globals/link",

                "extraAttributes": {
                    "_template": "/assets/base/globals/attributes",
                    "href"  : "link"
                },

                "body": {

                    "_template": "/assets/base/globals/image",

                    "extraAttributes": {
                        "_template": "/assets/base/globals/attributes",
                        "alt": "{{words([ 1, 2 ])}}",
                        "src": "{{image(50, 50)}}"
                    }
                }
            },

            "profileName": {
                "_template": "/assets/base/globals/link",
                "extraAttributes": {
                    "href": "link-me",
                    "_template": "/assets/base/globals/attributes"
                },
                "body": {
                    "_template": "/assets/base/globals/text",
                    "text": "{{words(2)}}"
                }
            },

            "profileUser": {
                "_template": "/assets/base/globals/link",
                "extraAttributes": {
                    "href": "link-me",
                    "_template": "/assets/base/globals/attributes"
                },
                "body": {
                    "_template": "/assets/base/globals/text",
                    "text": "{{words(2)}}"
                }
            },

            "socialFeedBody": [
                {
                    "_template": "/assets/base/globals/image",

                    "extraAttributes": {
                        "_template": "/assets/base/globals/attributes",
                        "alt": "{{words([ 1, 2 ])}}",
                        "src": "{{image(80, 80)}}"
                    }
                },
                {
                    "_template": "/assets/base/globals/html",
                    "text": "{{words([ 10, 20 ])}}"
                }
            ],

            "publishDate": {
                "_template": "/assets/base/globals/text",
                "text": "Oct 24 5:30pm"
            },

            "publishLinks": [
                {
                    "_template": "/assets/base/globals/link",
                    "cssClass": "icon-share",
                    "extraAttributes": {
                        "href": "share-link",
                        "_template": "/assets/base/globals/attributes"
                    }
                },
                {
                    "_template": "/assets/base/globals/link",
                    "cssClass": "icon-thumbs-up",
                    "extraAttributes": {
                        "href": "retweet-link",
                        "_template": "/assets/base/globals/attributes"
                    }
                },
                {
                    "_template": "/assets/base/globals/link",
                    "cssClass": "icon-comment",
                    "extraAttributes": {
                        "href": "star-link",
                        "_template": "/assets/base/globals/attributes"
                    }
                }
            ]
        },
        {
            "_template": "/assets/base/SocialFeedItem",

            "_repeat": 3,

            "feedType": "twitter",

            "profilePhoto": {
                "_template": "/assets/base/globals/link",

                "extraAttributes": {
                    "_template": "/assets/base/globals/attributes",
                    "href"  : "link"
                },

                "body": {

                    "_template": "/assets/base/globals/image",

                    "extraAttributes": {
                        "_template": "/assets/base/globals/attributes",
                        "alt": "{{words([ 1, 2 ])}}",
                        "src": "{{image(50, 50)}}"
                    }
                }
            },

            "profileName": {
                "_template": "/assets/base/globals/link",
                "extraAttributes": {
                    "href": "link-me",
                    "_template": "/assets/base/globals/attributes"
                },
                "body": {
                    "_template": "/assets/base/globals/text",
                    "text": "{{words(2)}}"
                }
            },

            "socialFeedBody": [
                {
                    "_template": "/assets/base/globals/html",
                    "text": "{{words([ 20, 30 ])}}"
                }
            ],

            "publishDate": {
                "_template": "/assets/base/globals/text",
                "text": "Oct 24 5:30pm"
            },

            "publishLinks": [
                {
                    "_template": "/assets/base/globals/link",
                    "cssClass": "icon-share",
                    "extraAttributes": {
                        "href": "share-link",
                        "_template": "/assets/base/globals/attributes"
                    }
                },
                {
                    "_template": "/assets/base/globals/link",
                    "cssClass": "icon-retweet",
                    "extraAttributes": {
                        "href": "retweet-link",
                        "_template": "/assets/base/globals/attributes"
                    }
                },
                {
                    "_template": "/assets/base/globals/link",
                    "cssClass": "icon-star",
                    "extraAttributes": {
                        "href": "star-link",
                        "_template": "/assets/base/globals/attributes"
                    }
                }
            ]
        }
    ],

    "cta": {
        "_template": "/assets/base/globals/link",
        "body": "{{words}}",
        "extraAttributes": {
            "href": "#"
        }
    }
}
