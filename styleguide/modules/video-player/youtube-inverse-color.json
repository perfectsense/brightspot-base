{
    "modifierClass" : "color-inverse",

    "_template" : "/assets/base/video-player",

    "title" : {
        "_template" : "/assets/base/globals/text",
        "text" : "{{words(10)}}"
    },

    "description" : {
        "_template" : "/assets/base/globals/text",
        "text" : "{{words(20)}}"
    },

    "align" : "left",

    "youtube" : true,

    "src" : "https://www.youtube.com/embed/dQw4w9WgXcQ",

    "primaryTag" : {
      "_template": "/assets/base/tags",
      "tagList": [
        {
          "_template": "/assets/base/globals/link",
          "body": "{{words([ 1, 2 ])}}",
          "extraAttributes": {

            "href": "#"
          }
        }
      ]
    },

    "sharing" : {
        "_dataUrl": "/elements/social-share/share-monochrome.json"
    }

}
