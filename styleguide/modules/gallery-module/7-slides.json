{

    "_template": "/assets/base/gallery/gallery-module",
    "displayOptions": {
        "dots": true
    },
    "heading": {
        "_template": "/assets/base/globals/text",
        "text": "7 slides"
    },
    "gallerySlides": {
        "_template": "/assets/base/gallery/gallery-module-slide-content",
        "content": [{
            "_template": "/assets/base/globals/image",
            "_repeat": 7,
            "extraAttributes": {

                "src": "{{image([ 600, 1200, 100 ], 400)}}"
            }
        }]
    },
    "cta": {
        "_template": "/assets/base/globals/link",
        "extraAttributes": {
            "href": "link-me",
            "title": "I are a link",
            "_template": "/assets/base/globals/attributes"
        },
        "body": {
            "_template": "/assets/base/globals/text",
            "text": "I am a CTA Link Test"
        }
    }
}
