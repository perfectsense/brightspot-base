{

  "_template": "/assets/templates/base/modules/gallery-module-with-nav",

  "displayOptions" : {
    "thumbnailsToShow": "5"
    },

  "heading": {
    "_template": "/assets/templates/base/common/text",
    "text": "5 slides with thumbs"
  },
  "gallerySlides": {
    "_template": "/assets/templates/base/modules/gallery-module-slide-content",

    "content" : [{
      "_template": "/assets/templates/base/common/image",
      "_repeat": 5,
      "extraAttributes": {
        "src": "{{image(1200, 600)}}",
        "_template": "/assets/templates/base/common/attributes"
      }
    }]
  },

  "galleryNavItems": {
    "_template": "/assets/templates/base/modules/gallery-module-slide-content",

    "content" : [{
      "_template": "/assets/templates/base/common/image",
      "_repeat": 5,
      "extraAttributes": {
        "src": "{{image(250, 150)}}",
        "_template": "/assets/templates/base/common/attributes"
      }
    }]
  }

}
