{
    "_template": "/assets/base/gallery-module",

    "displayOptions": {
        "dynamicSlideLoad": true,
        "dynamicEndpoint": "_dynamic-slides",
        "dynamicCount" : 5
    },

    "heading": {
        "_template": "/assets/base/globals/text",
        "text": "Dynamic slides, no dots, no thumbs"
    },

    "gallerySlides": {

        "_template": "/assets/base/gallery-module-slide-content",

        "content": [{
            "_template": "/assets/base/globals/image",

            "extraAttributes": {
                "src": "{{image(1200, 600)}}",
                "title": "slide-0"
            }
        }]

    },
    "cta": {
        "_template": "/assets/base/globals/link",
        "extraAttributes": {
            "href": "link-me",
            "title": "I are a link",
            "_template": "/assets/base/globals/attributes"
        },
        "body": {
            "_template": "/assets/base/globals/text",
            "text": "I am a CTA Link Test"
        }
    }
}
