{

    "_template": "/assets/base/gallery-module",

    "displayOptions": {
        "horizontalSlides": true,
        "slidesToShowMedium": 5,
        "slidesToShowSmall": 2,
        "slidesToScrollMedium": 5,
        "slidesToScrollSmall": 2,
        "theme": "horizontal"
    },

    "heading": {
        "_template": "/assets/base/globals/text",
        "text": "5 slides at a time, 2 on mobile"
    },

    "gallerySlides": {
        "_template": "/assets/base/gallery-module-slide-content",

        "content": [{
            "_template": "/assets/base/globals/image",
            "_repeat": 5,
            "extraAttributes": {

                "src": "{{image(600, 400)}}"
            }
        }]
    }
}
