{

    "_template": "/assets/base/gallery-module-with-nav",

    "displayOptions": {
        "textNav": true
    },

    "heading": {
        "_template": "/assets/base/globals/text",
        "text": "4 Slide with text nav instead of thumbs"
    },
    "gallerySlides": {
        "_template": "/assets/base/gallery-module-slide-content",

        "content": [{
            "_template": "/assets/base/globals/image",
            "_repeat": 4,
            "extraAttributes": {

                "src": "{{image(800, 400)}}"
            }
        }]
    },
    "galleryNavItems": {
        "_template": "/assets/base/gallery-module-slide-content",

        "content": [{
            "_template": "/assets/base/globals/text",
            "_repeat": 17,
            "text": "{{words(10)}}"
        }]
    }
}
