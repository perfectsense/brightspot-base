{

    "_template": "/assets/templates/base/modules/gallery-full",

    "displayOptions" : {
        "modifierClass" : "component-inverse-color"
    },

    "heading": {
        "_template" : "/assets/templates/base/common/text",
        "text" : "{{words(10)}}"
    },

    "description" : {
        "_template" : "/assets/templates/base/common/text",
        "text" : "{{words(20)}}"
    },

    "slides": [
        {
            "_repeat" : 5,
            "_template" : "/assets/templates/base/modules/gallery-full-slide-content",
            "image" : {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {

                    "src": "{{image(1600, 900)}}"
                }
            },
            "caption" : [
                {
                    "_template": "/assets/templates/base/common/text",
                    "text" : "{{words(20)}}"
                }
            ],
            "sharing" : {
                "_dataUrl" : "/elements/social-share/share.json"
            }
        },
        {
            "_template" : "/assets/templates/base/modules/gallery-full-interstitial",
            "displayOptions" : {
                "countDisable" : false,
                "countMessage" : "Advertisement: You may skip in {n} seconds",
                "countDuration" : 5,
                "countPosition" : "topRight",
                "navEnabled" : false,
                "navVisible" : true,
                "modifierClass" : "interstitial"
            },
            "contentUrl" : "_interstitial.html"
        },
        {
            "_repeat" : 5,
            "_template" : "/assets/templates/base/modules/gallery-full-slide-content",
            "image" : {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {

                    "src": "{{image(1600, 900)}}"
                }
            },
            "caption" : [
                {
                    "_template": "/assets/templates/base/common/text",
                    "text" : "{{words(20)}}"
                }
            ],
            "sharing" : {
                "_dataUrl" : "/elements/social-share/share.json"
            }
        }
    ],

    "showMoreText" : {
        "_template": "/assets/templates/base/common/text",
        "text" : "Show More"
    },

    "showLessText" : {
        "_template": "/assets/templates/base/common/text",
        "text" : "Show Less"
    }
}
