{


    "_template" : "/assets/base/SearchResults",

    "loadMore" : true,

    "heading" : {
        "_template" : "/assets/base/ModuleGroup",

        "modules" : [
            {
                "_template" : "/assets/base/globals/text",
                "text" : "{{words(5)}}"
            },
            {
                "_template" : "/assets/base/globals/text",
                "displayOptions" : {
                    "modifierClass" : "SearchResults-term"
                },
                "text" : "\"{{words(1)}}\""
            }
        ],

        "searchResultsSort" : {
            "_template": "/assets/base/forms/form",

            "displayOptions" : {
                "modifierClass": "SearchResults-sort"
            },

            "extraAttributes" : {
                "action" : ""
            },

            "inputs" : [
                {

                    "_template" : "/assets/base/forms/form-select",

                    "autoSubmit" : true,

                    "displayOptions" : {
                        "styledSelect" : true
                    },

                    "extraAttributes": {
                      "id": "SearchResults-sort"
                    },

                    "label" : {
                        "_template": "/assets/base/forms/form-label-attributes",

                        "extraAttributes" : {
                            "for": "SearchResults-sort"
                        }
                    },

                    "labelText" : {
                        "_template" : "/assets/base/globals/text",
                        "text": "Sort by:"
                    },

                    "optionsData": [
                      {
                        "_template" : "/assets/base/forms/form-select-option",

                        "_repeat" : 3,

                        "extraAttributes": {
                          "value": "{{words(1)}}"
                        },
                        "text": {
                          "_template": "/assets/base/globals/text",
                          "text" : "{{words(1)}}"
                        }
                      }
                    ]
                }
            ]
        }
    },

    "searchResults" : {
      "_template" : "/assets/base/promo",

      "displayOptions" : {
        "modifierClass" : "promo-small",
        "horizontalSplit" : "third"
      },

      "listItems" : [
          {
              "_template": "/assets/base/promo-item",

              "_repeat" : 9,

              "sections" : [
                  {

                      "_template" : "/assets/base/promo-section",

                      "type" : "tags",

                      "tags" : {

                          "_template": "/assets/base/Tags",
                          "tagList": [
                              {
                                  "_template": "/assets/base/globals/link",
                                  "extraAttributes": {
                                      "href": "#",
                                      "_template": "/assets/base/globals/attributes"
                                  },
                                  "body": {
                                      "_template": "/assets/base/globals/text",
                                      "text": "{{words(2)}}"
                                  }
                              }
                          ]
                      }
                  },
                  {

                      "_template" : "/assets/base/promo-section",

                      "type" : "image",

                      "image" : {

                          "_template" : "/assets/base/globals/link",

                          "extraAttributes" : {
                              "_template" : "/assets/base/globals/attributes",
                              "href"  : "link"
                          },

                          "body" : {

                              "_template" : "/assets/base/globals/image",

                              "extraAttributes" : {
                                  "_template" : "/assets/base/globals/attributes",
                                  "alt" : "{{words([ 1, 2 ])}}",
                                  "src" : "{{image(800, 450)}}"
                              }
                          }
                      }
                  },
                  {

                      "_template" : "/assets/base/promo-section",

                      "type" : "text",

                      "title" : {
                          "_template" : "/assets/base/globals/link",

                          "extraAttributes" : {
                              "_template" : "/assets/base/globals/attributes",
                              "href"  : "#"
                          },

                          "body" : {
                              "_template" : "/assets/base/globals/text",
                              "text" : "{{words([ 1, 2 ])}}"
                          }
                      },

                      "description" : {
                          "_template" : "/assets/base/globals/text",

                          "text" : "{{words(10)}}"
                      }

                  }

              ]
          }
      ],

      "cta": {
          "_template": "/assets/base/globals/link",

          "cssClass" : "load-more",

          "extraAttributes": {
              "href": "index",
              "data-ajax-link-target" : ".SearchResults-content"
          },

          "body": {
              "_template": "/assets/base/globals/text",
              "text": "{{words(2)}}"
          }
      }
  }

}
