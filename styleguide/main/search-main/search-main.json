{
    "_template": "base/main/SearchMain",

    "headline": "{{words(4)}}",
    "byline": "{{number(10)}} results",
    "submitAction": ".?q=keywords&offset=0&limit=3",

    "filter": {
        "_template": "base/form/Select",

        "id": "filter-123",
        "label": "filter",
        "autosubmit": true,
        "options": [
            {
                "_repeat": [2,10],
                "_template": "base/form/Option",

                "text": "{{words(2)}}",
                "value": "{{words(2)}}"
            }
        ]
    },

    "sort": {
        "_template": "base/form/Select",

        "id": "sort-123",
        "label": "sort",
        "autosubmit": true,
        "options": [
            {
                "_repeat": [2,10],
                "_template": "base/form/Option",

                "text": "{{words(2)}}",
                "value": "{{words(2)}}"
            }
        ]
    },

    "body": {
        "_template": "base/main/SearchMainResult",

        "items": {
            "_template": "base/util/Concatenated",

            "items": [
                {
                    "_repeat": 9,
                    "_template": "base/util/RawHtml",

                    "html": "<div>{{words(3)}}</div>"
                }
            ]
        },

        "loadMore": true,
        "loadMoreAction": "_search-main-result-response?q=keywords&offset={{number([1,10])}}&limit=3",
        "loadMoreLabel": "Load More"
    }
}
