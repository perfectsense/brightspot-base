{
    "_template" : "/assets/base/globals/head",

    "title" : "Styleguide",
    "description" : "Inspire Confidence Styleguide",
    "keywords" : "Brightspot Base, Styleguide, Inspire Confidence",
    "canonicalLink" : "http://styleguide.inspireconfidence.psdops.com",

    "metas" : [
      {
        "_template": "/assets/base/globals/meta",
        "extraAttributes": {
          "http-equiv" : "X-UA-Compatible",
          "content" : "IE=edge"
        }
      },
      {
        "_template": "/assets/base/globals/meta",
        "extraAttributes": {
          "name" : "viewport",
          "content" : "width=device-width, initial-scale=1"
        }
      }
    ],

    "icons" : [
      {
        "_template": "/assets/base/globals/icon",
        "extraAttributes": {
          "rel": "shortcut icon",
          "href": "/favicon.ico"
        }
      }
    ],

    "styles" : [
      {
        "_template" : "/assets/base/globals/style",
        "extraAttributes": {
          "rel" : "stylesheet",
          "type" : "text/css",
          "href" : "/assets/main.min.css"
        }
      }, {
        "_template" : "/assets/base/globals/style",
        "extraAttributes": {
          "rel" : "stylesheet",
          "type" : "text/css",
          "href" : "http://fonts.googleapis.com/css?family=Roboto:700,400,300,100"
        }
      }, {
        "_template" : "/assets/base/globals/style",
        "content" : {
          "_template" : "/assets/base/globals/html",
          "text" : ".foo { text-align: center; content: 'hello & world' }"
        }
      }
    ],

    "scripts" : [
      {
        "_template" : "/assets/base/globals/script",
        "extraAttributes": {
          "type" : "text/javascript",
          "src": "/assets/main.min.js"
        }
      }, {
        "_template": "/assets/base/globals/script",
        "extraAttributes": {
          "type" : "text/javascript"
        },
        "content" : {
          "_template" : "/assets/base/globals/html",
          "text" : "console.log('Hello & World!');"
        }
      }
    ]
}
