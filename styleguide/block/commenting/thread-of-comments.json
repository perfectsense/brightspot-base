{
  "_template": "base/Commenting",
  "extraAttributes": {
    "data-bsp-community-commenting-ajaxMethod": "GET"
  },
  "expandComments": true,
  "header": {
    "_template": "base/CommentingHeader",
    "title": "25 comments"
  },
  "body": {
    "_template": "base/util/Concatenated",
    "items": [
      {
        "_repeat": [
          1,
          3
        ],
        "_dataUrl": "/block/commenting/comment.json"
      },
      {
        "_repeat": [
          1,
          3
        ],
        "_dataUrl": "/block/commenting/comment.json",
        "threadDepth": "1",
        "parentId": "1",
        "id": "{{number([2,20])}}"
      },
      {
        "_repeat": [
          1,
          3
        ],
        "_dataUrl": "/block/commenting/comment.json",
        "threadDepth": "0",
        "id": "{{number([30,40])}}"
      }
    ]
  },
  "actions": {
    "_template": "base/util/Concatenated",
    "items": [
      {
        "_dataUrl": "/block/commenting/comment-submit.json"
      },
      {
        "_template": "base/CommentingShowMoreButton",
        "ajaxTarget": "_ajax-show-more-response.html",
        "body": "Show More Comments"
      }
    ]
  }
}
