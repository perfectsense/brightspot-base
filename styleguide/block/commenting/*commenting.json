{
  "_template": "base/commenting/Commenting",
  "id": "324kldsjlj34",
  "expandComments": true,
  "extraAttributes": {
    "data-bsp-community-commenting-ajaxMethod": "GET"
  },
  "header": {
    "_template": "base/commenting/CommentingHeader",
    "title": "30 Comments"
  },
  "body": {
    "_template": "base/util/Concatenated",
    "items": [
      {
        "_repeat": [
          1,
          3
        ],
        "_dataUrl": "/block/commenting/comment.json"
      },
      {
        "_repeat": [
          1,
          3
        ],
        "_dataUrl": "/block/commenting/comment.json",
        "threadDepth": "1",
        "parentId": "1",
        "id": "{{number([2,20])}}",
        "isStaff": true
      },
      {
        "_repeat": [
          1,
          3
        ],
        "_dataUrl": "/block/commenting/comment.json",
        "threadDepth": "0",
        "id": "{{number([30,40])}}"
      }
    ]
  },
  "actions": {
    "_template": "base/util/Concatenated",
    "items": [
      {
        "_dataUrl": "/block/commenting/comment-entry.json"
      },
      {
        "_template": "base/commenting/CommentingShowMore",
        "ajaxTarget": "_ajax-show-more-response.html",
        "button": "Show More Comments"
      }
    ]
  }
}
