{
  "_wrapper": false,
  "_template": "community/commenting/CommentLoadMoreResponse",

  "comments": {
    "_template": "base/util/Concatenated",
    "items": [
      {
        "_repeat": [
          1,
          5
        ],
        "_template": "community/commenting/Comment",
        "id": "sdf43235dwf3dsfsd4",
        "isStaff": false,
        "threadDepth": "0",
        "author": {
          "_dataUrl": "/block/author/author.json"
        },
        "time": {
          "_template": "base/util/DateTime",
          "iso": "{{date('iso')}}",
          "body": "{{date()}}"
        },
        "body": {
          "_template": "base/util/RawHtml",
          "html": "{{paragraphs(1)}}"
        },
        "replyButton": "Reply",
        "replyAction": "/block/commenting/_comment-entry-response"
      }
    ]
  },

  "loadMoreButton": "Show More",
  "loadMoreAction": "/block/commenting/_loadMore-response?page=2"
}
