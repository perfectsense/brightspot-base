{
    "_wrapper": false,
    "_template": "/base/page/Page.hbs",

    "head": {
        "_template": "/base/page/Head.hbs",
        "items": [
            {
                "_template": "/base/page/Meta.hbs",
                "httpEquiv": "Content-Language",
                "content": "en"
            },
            {
                "_template": "/base/page/Meta.hbs",
                "name": "description",
                "content": "{{words}}"
            },
            {
                "_template": "/base/page/Meta.hbs",
                "property": "og:type",
                "content": "website"
            }
        ]
    },

    "body": {
        "_template": "/base/util/RawHtml.hbs",
        "html": "{{paragraphs}}"
    }
}
