{{#block "TwitterPlayerCard" extend="TwitterCard.hbs"}}
    {{#blockBody}}
        {{#element "card" noWith="true"}}
            <meta name="twitter:card" content="player"/>
        {{/element}}

        {{element "description"}}
        {{element "image"}}
        {{element "imageAlt"}}
        {{element "player"}}
        {{element "playerHeight"}}
        {{element "playerStream"}}
        {{element "playerStreamContentType"}}
        {{element "playerWidth"}}
        {{element "site"}}
        {{element "siteId"}}
        {{element "title"}}
    {{/blockBody}}
{{/block}}
