{{#block "TwitterSummaryCard" extend="TwitterCard.hbs"}}
    {{#blockBody}}
        {{#element "card" noWith="true"}}
            <meta name="twitter:card" content="summary"/>
        {{/element}}

        {{element "creator"}}
        {{element "creatorId"}}
        {{element "description"}}
        {{element "image"}}
        {{element "imageAlt"}}
        {{element "site"}}
        {{element "siteId"}}
        {{element "title"}}
    {{/blockBody}}
{{/block}}

