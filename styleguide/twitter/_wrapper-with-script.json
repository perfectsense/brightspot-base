{
    "_template": "../core/page/Page.hbs",

    "title": "Styleguide",
    "description": "Brightspot Twitter Base Styleguide",

    "headItems": {
        "_template": "../core/Concatenated.hbs",

        "items": [
            {
                "_template": "../core/page/PageMeta.hbs",
                "httpEquiv": "X-UA-Compatible",
                "content": "IE=edge"
            },
            {
                "_template": "../core/page/PageMeta.hbs",
                "name": "viewport",
                "content": "width=device-width, initial-scale=1"
            },
            {
                "_dataUrl": "widgets/TwitterWidgetsJavaScript.json"
            }
        ]
    },

    "body": {
        "_delegate": true
    }
}
