{{#block "OpenGraphAudioMeta"}}
    {{#blockBody}}
        {{! Required Elements }}

        {{#element "url"}}
            <meta property="og:audio" content="{{this}}" />
        {{/element}}

        {{! Optional Elements }}

        {{#element "secureUrl"}}
            <meta property="og:audio:secure_url" content="{{this}}" />
        {{/element}}

        {{#element "type"}}
            <meta property="og:audio:type" content="{{this}}" />
        {{/element}}
    {{/blockBody}}
{{/block}}
