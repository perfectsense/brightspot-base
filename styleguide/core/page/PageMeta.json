{
    "_wrapper": false,
    "_template": "Page.hbs",

    "headItems": {
        "_template": "../Concatenated.hbs",

        "items": [
            {
                "_template": "PageMeta.hbs",
                "httpEquiv": "Content-Language",
                "content": "en"
            },
            {
                "_template": "PageMeta.hbs",
                "name": "description",
                "content": "{{words}}"
            },
            {
                "_template": "PageMeta.hbs",
                "property": "og:type",
                "content": "website"
            }
        ]
    },

    "body": {
        "_template": "../RawHtml.hbs",
        "html": "{{paragraphs}}"
    }
}
