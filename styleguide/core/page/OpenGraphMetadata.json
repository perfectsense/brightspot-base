{
    "_wrapper": false,
    "_template": "../page/Page.hbs",

    "title": "Styleguide - Open Graph",
    "description": "Brightspot Base Styleguide - Open Graph",
    "keywords": "Brightspot Base, Styleguide, Inspire Confidence, Open Graph",

    "headItems": {
        "_template": "../Concatenated.hbs",

        "items": [
            {
                "_template": "../page/PageMeta.hbs",
                "httpEquiv": "X-UA-Compatible",
                "content": "IE=edge"
            },
            {
                "_template": "../page/PageMeta.hbs",
                "name": "viewport",
                "content": "width=device-width, initial-scale=1"
            },
            {
                "_template": "OpenGraphMetadata.hbs",
                "title": "{{words(5)}}",
                "type": "website",
                "url": "http://brightspot.com",
                "description": "{{words([5, 15])}}",
                "determiner": "auto",
                "locale": "en_US",
                "localeAlternates": [
                    "es_ES",
                    "fr_FR"
                ],
                "siteName": "Brightspot Base",
                "images": [
                    {
                        "_dataUrl": "_OpenGraphImageMetadata.json",
                        "_repeat": [1, 4]
                    }
                ],
                "videos": [
                    {
                        "_dataUrl": "_OpenGraphVideoMetadata.json",
                        "_repeat": [1, 4]
                    }
                ],
                "audios": [
                    {
                        "_dataUrl": "_OpenGraphAudioMetadata.json",
                        "_repeat": [1, 4]
                    }
                ]
            }
        ]
    },

    "body": ""
}