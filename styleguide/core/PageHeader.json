{
    "_template": "PageHeader.hbs",

    "logo": {
        "_template": "Image.hbs",
        "src": "{{image(200, 60)}}"
    },

    "navigation": {
        "_dataUrl": "PageNavigation.json"
    },

    "search": {
        "_template": "Form.hbs",

        "actionUrl": "{{var('exampleSiteUrl')}}",
        "items": [
            {
                "_template": "Input.hbs",
                "placeholder": "Search"
            }

        ],
        "actionButton": {
            "_template": "Button.hbs",
            "text": "Go"
        }
    }
}
