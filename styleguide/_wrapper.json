{
    "_template": "/base/page/Page",

    "head": {
        "_template": "/base/page/Head",
        "title": "Styleguide",
        "description": "Brightspot Base Styleguide",
        "keywords": "Brightspot Base, Styleguide, Inspire Confidence",
        "items": [
            {
                "_template": "/base/page/Meta",
                "httpEquiv": "X-UA-Compatible",
                "content": "IE=edge"
            },
            {
                "_template": "/base/page/Meta",
                "name": "viewport",
                "content": "width=device-width, initial-scale=1"
            },
            {
                "_template": "/base/page/ExternalStylesheet",
                "href": "{{stylesheet}}"
            },
            {
                "_template": "/base/page/ExternalScript",
                "src": "/main.min.js"
            }
        ]
    },

    "body": {
        "_delegate": true
    }
}
