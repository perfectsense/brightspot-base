{
    "_template": "/assets/base/page/StandardPage",

    "head": {
        "_template": "/assets/base/page/StandardHead",
        "title": "Styleguide",
        "description": "Brightspot Base Styleguide",
        "keywords": "Brightspot Base, Styleguide, Inspire Confidence",
        "items": [
            {
                "_template": "/assets/base/page/Meta",
                "httpEquiv": "X-UA-Compatible",
                "content": "IE=edge"
            },
            {
                "_template": "/assets/base/page/Meta",
                "name": "viewport",
                "content": "width=device-width, initial-scale=1"
            },
            {
                "_template": "/assets/base/page/ExternalStyle",
                "href": "{{stylesheet}}"
            }
        ]
    },

    "body": {
        "_delegate": true
    }
}
