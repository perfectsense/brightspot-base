{
  "_template": "components/bsp-form-input",
  "showMessagesAbove": true,
  "attributes": {
    "autocomplete": "off",
    "data-bsp-form-constraint-matches": "#pw1",
    "id": "pw2",
    "required": true,
    "type": "password",
    "_template": "common/attributes"
  },
  "errorMessages": {
    "_template": "common/json-object",
    "constraintMatches": "Passwords must match",
    "valueMissing": "Please enter your password again"
  },
  "label": {
    "_template": "common/text",
    "text": "Password (repeat)"
  }
}
