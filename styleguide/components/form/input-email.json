{
  "_template": "components/bsp-form-input",
  "showMessagesAbove": true,
  "attributes": {
    "autocomplete": "off",
    "id": "emailinput1",
    "placeholder": "Email input placeholder",
    "required": true,
    "type": "email",
    "_template": "common/attributes"
  },
  "errorMessages": {
    "_template": "common/json-object",
    "typeMismatch": "Email address must follow format a@b.c",
    "valueMissing": "Please enter an email address"
  },
  "labelAttributes": {
    "for": "emailinput1",
    "_template": "common/attributes"
  },
  "label": {
    "_template": "common/text",
    "text": "Email input"
  }
}
