{
  "_template": "components/bsp-bio",
  "image": {
    "_template": "common/image",
    "attributes": {
      "src": "{{image(200, 300)}}",
      "_template": "common/attributes"
    }
  },
  "title": "{{name}}",
  "subtitles": [
    {
      "_template": "common/link",
      "_repeat": [ 1, 3 ],
      "body": "{{words([ 1, 3 ])}}",
      "attributes": {
        "_template": "common/attributes",
        "href": "#"
      }
    }
  ],
  "text": {
    "_template": "common/html",
    "text": "{{paragraphs(1)}}"
  },
  "primaryList": [
    {
      "_template": "components/bsp-bio-list-item",
      "_repeat": [ 1, 5 ],
      "heading": "{{words([ 1, 3 ])}}",
      "text": "{{words([ 1, 5 ])}}"
    }
  ],
  "secondaryList": [
    {
      "_template": "components/bsp-bio-list-item",
      "_repeat": [ 1, 5 ],
      "heading": "{{words([ 1, 3 ])}}",
      "text": "{{words([ 1, 5 ])}}"
    }
  ],
  "socialLinks": [
    {
      "_template": "components/bsp-bio-social-link",
      "icon": "twitter",
      "link": {
        "_dataUrl": "/common/link-test-text-14-char.json"
      }
    },
    {
      "_template": "components/bsp-bio-social-link",
      "icon": "facebook",
      "link": {
        "_dataUrl": "/common/link-test-text-14-char.json"
      }
    },
    {
      "_template": "components/bsp-bio-social-link",
      "icon": "instagram",
      "link": {
        "_dataUrl": "/common/link-test-text-14-char.json"
      }
    },
    {
      "_template": "components/bsp-bio-social-link",
      "icon": "external-link",
      "link": {
        "_dataUrl": "/common/link-test-text-14-char.json"
      }
    }
  ],
  "extraImages": [
    {
      "_template": "common/image",
      "attributes": {
        "alt": "image",
        "src": "/assets/images/200x300.png",
        "_template": "common/attributes"
      }
    },
    {
      "_template": "common/image",
      "attributes": {
        "alt": "image",
        "src": "/assets/images/200x300.png",
        "_template": "common/attributes"
      }
    },
    {
      "_template": "common/image",
      "attributes": {
        "alt": "image",
        "src": "/assets/images/200x300.png",
        "_template": "common/attributes"
      }
    }
  ]
}
