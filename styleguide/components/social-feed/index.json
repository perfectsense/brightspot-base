{
    "_wrapper": "/_wrapper.json",
    "_template" : "/assets/templates/base/components/social-feed",

    "displayOptions" : {
        "horizontalSplit" : "third"
    },

    "heading" : {
        "_template" : "/assets/templates/base/components/component-group",

        "components" : [
            {
                "_template" : "/assets/templates/base/common/link",

                "attributes" : {
                    "_template" : "/assets/templates/base/common/attributes",
                    "href"  : "link"
                },

                "body" : {
                    "_template" : "/assets/templates/base/common/text",
                    "text" : "{{words(5)}}"
                }
            },
            {

                "_template" : "/assets/templates/base/common/text",

                "displayOptions" : {
                    "modifierClass" : "component-subtitle"
                },

                "text" : "{{words(20)}}"

            }
        ]
    },

    "items" : [
        {
            "_template": "/assets/templates/base/components/social-feed-item",

            "_repeat" : 2,

            "feedType" : "facebook",

            "profilePhoto" : {
                "_template" : "/assets/templates/base/common/link",

                "attributes" : {
                    "_template" : "/assets/templates/base/common/attributes",
                    "href"  : "link"
                },

                "body" : {

                    "_template" : "/assets/templates/base/common/image",

                    "attributes" : {
                        "_template" : "/assets/templates/base/common/attributes",
                        "alt" : "{{words([ 1, 2 ])}}",
                        "src" : "{{image(50, 50)}}"
                    }
                }
            },

            "profileName" : {
                "_template": "/assets/templates/base/common/link",
                "attributes": {
                    "href": "link-me",
                    "_template": "/assets/templates/base/common/attributes"
                },
                "body": {
                    "_template": "/assets/templates/base/common/text",
                    "text": "{{words(2)}}"
                }
            },

            "profileUser" : {
                "_template": "/assets/templates/base/common/link",
                "attributes": {
                    "href": "link-me",
                    "_template": "/assets/templates/base/common/attributes"
                },
                "body": {
                    "_template": "/assets/templates/base/common/text",
                    "text": "{{words(2)}}"
                }
            },

            "socialFeedBody" : [
                {
                    "_template" : "/assets/templates/base/common/image",

                    "attributes" : {
                        "_template" : "/assets/templates/base/common/attributes",
                        "alt" : "{{words([ 1, 2 ])}}",
                        "src" : "{{image(100, 100)}}"
                    }
                },
                {
                    "_template": "/assets/templates/base/common/html",
                    "text": "{{words([ 10, 20 ])}}"
                }
            ],

            "publishDate" : {
                "_template": "/assets/templates/base/common/text",
                "text": "Oct 24 5:30pm"
            }
        },
        {
            "_template": "/assets/templates/base/components/social-feed-item",

            "feedType" : "twitter",

            "profilePhoto" : {
                "_template" : "/assets/templates/base/common/link",

                "attributes" : {
                    "_template" : "/assets/templates/base/common/attributes",
                    "href"  : "link"
                },

                "body" : {

                    "_template" : "/assets/templates/base/common/image",

                    "attributes" : {
                        "_template" : "/assets/templates/base/common/attributes",
                        "alt" : "{{words([ 1, 2 ])}}",
                        "src" : "{{image(50, 50)}}"
                    }
                }
            },

            "profileName" : {
                "_template": "/assets/templates/base/common/link",
                "attributes": {
                    "href": "link-me",
                    "_template": "/assets/templates/base/common/attributes"
                },
                "body": {
                    "_template": "/assets/templates/base/common/text",
                    "text": "{{words(2)}}"
                }
            },

            "socialFeedBody" : [
                {
                    "_template": "/assets/templates/base/common/html",
                    "text": "{{words([ 20, 30 ])}}"
                }
            ]
        }
    ],

    "cta": {
        "_template": "/assets/templates/base/components/component-cta",
        "content": {
            "_template": "/assets/templates/base/common/link",
            "attributes": {
                "href": "link-me",
                "_template": "/assets/templates/base/common/attributes"
            },
            "body": {
                "_template": "/assets/templates/base/common/text",
                "text": "{{words(2)}}"
            }
        }
    }
}
