{
  "_template": "components/bsp-search-results",
  "heading": {
    "_template": "common/html",
    "text": "We found <span class='bsp-results'>128</span> results for <span class='bsp-search-query'>\"Lion\"</span>"
  },
  "searchForm": {
    "attributes": {
      "action": "/components/bsp-search-results/search-results-ajax-1.html?q=test",
      "_template": "common/attributes"
    },
    "searchInput": {
      "_template": "components/bsp-form-input",
      "showMessagesAbove": true,
      "attributes": {
        "autocomplete": "off",
        "class": "bsp-search-results-input",
        "id": "q",
        "name": "q",
        "required": true,
        "type": "text",
        "value": "Lion",
        "_template": "common/attributes"
      },
      "errorMessages": {
        "_template": "common/json-object",
        "valueMissing": "Please fill in the search query"
      }
    }
  },
  "searchSort": {
    "_dataUrl": "/components/bsp-search-results/search-sort.json"
  },
  "searchFilter": {
    "_dataUrl": "/components/bsp-search-results/search-filter.json"
  },
  "searchResults": [
    {
      "_dataUrl": "/components/bsp-search-results/results-show-lead.json"
    },
    {
      "_dataUrl": "/components/bsp-search-results/results-show.json"
    },
    {
      "_dataUrl": "/components/bsp-search-results/results-images.json"
    },
    {
      "_dataUrl": "/components/bsp-search-results/results-theater.json"
    },
    {
      "_dataUrl": "/components/bsp-search-results/results-show.json"
    }
  ],
  "pagination": {
    "_dataUrl": "/components/bsp-pagination/3-pages.json"
  }
}
