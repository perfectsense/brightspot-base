{
  "_template": "components/gallery-module",
  "dynamicSlideLoad": true,
  "galleryTitle": {
    "_template": "common/text",
    "text": "Dynamic slides, no dots, no thumbs"
  },
  "gallerySlides": [
    {
      "_template": "components/gallery-module-slide-content",
        "content": {
            "_template": "common/image",
            "attributes": {
                "_template": "common/attributes",
                "src": "{{image(1200, 400)}}",
                "title": "slide-0",
                "data-next-url": "/styleguide/components/gallery-module/_dynamic-slide-1?ajax=true",
                "data-prev-url": "/styleguide/components/gallery-module/_dynamic-slide-neg-1?ajax=true"
            }
        }
    }
  ],
  "cta": {
    "_template": "components/component-cta",
    "location": "bottom",
    "content": {
      "_template": "common/link",
      "attributes": {
        "href": "link-me",
        "title": "I are a link",
        "_template": "common/attributes"
      },
      "body": {
        "_template": "common/text",
        "text": "I am a CTA Link Test"
      }
    }
  }
}
