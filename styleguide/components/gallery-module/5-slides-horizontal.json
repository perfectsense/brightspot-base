{
  "_template": "components/gallery-module",
  "horizontalSlides": true,
  "slidesToShowMedium": 5,
  "slidesToShowSmall": 2,
  "slidesToScrollMedium": 5,
  "slidesToScrollSmall": 2,
  "theme": "horizontal",
  "galleryTitle": {
    "_template": "common/text",
    "text": "5 slides at a time, 2 on mobile"
  },
  "gallerySlides": [
    {
      "_template": "components/gallery-module-slide-content",
        "_repeat": 5,
        "content": {
            "_template": "common/image",
            "attributes": {
                "_template": "common/attributes",
                "src": "{{image(600, 400)}}"
            }
        }
    }
  ]
}
