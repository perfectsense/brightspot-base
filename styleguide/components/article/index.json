{
  "_template": "components/bsp-article",
  "sharing": {
    "_dataUrl": "/components/social-share/share-notitle.json"
  },
  "permalink": "/bsp-article/",
  "publishDate": "05 Apr 2015",
  "title": "{{words}}",
  "subTitle": "{{words}}",
  "authors": [
    {
      "_template": "common/link",
      "_repeat": [ 1, 5 ],
      "body": "{{name}}",
      "attributes": {
        "_template": "common/attributes",
        "href": "#",
        "title": "{{name}}"
      }
    }
  ],
  "tags": {
    "_dataUrl": "/components/tags/tag-list.json"
  },
  "lead": {
    "_template": "common/figure",
    "credit": "{{name}}",
    "caption": {
      "_template": "common/text",
      "text": "{{words}}"
    },
    "image": {
      "_template": "common/image",
      "attributes": {
        "_template": "common/attributes",
        "src": "{{image(800, 450)}}"
      }
    }
  },
  "articleBody": [
    {
      "_template": "common/html",
      "text": "{{paragraphs}}"
    },
    {
      "_template": "components/bsp-enhancement",
      "content": {
        "_template": "components/bsp-blockquote",
        "author": "{{name}}",
        "content": "{{sentences}}"
      }
    },
    {
      "_template": "common/html",
      "text": "{{paragraphs}}"
    },
    {
      "_template": "components/bsp-enhancement",
      "content": {
        "_dataUrl": "/components/gallery-module/7-slides-dots.json"
      }
    },
    {
      "_template": "common/html",
      "text": "{{paragraphs}}"
    },
    {
      "_template": "components/bsp-enhancement",
      "align": "left",
      "content": {
        "_template": "common/image",
        "attributes": {
          "_template": "common/attributes",
          "src": "{{image([ 200, 300, 10 ], [ 200, 300, 10 ])}}"
        }
      }
    },
    {
      "_template": "common/html",
      "text": "{{paragraphs}}"
    },
    {
      "_template": "components/bsp-enhancement",
      "align": "right",
      "content": {
        "_template": "common/figure",
        "credit": "{{name}}",
        "image": {
          "_template": "common/image",
          "attributes": {
            "_template": "common/attributes",
            "src": "{{image([ 200, 300, 10 ], [ 200, 300, 10 ])}}"
          }
        }
      }
    },
    {
      "_template": "common/html",
      "text": "{{paragraphs}}"
    }
  ]
}
