{
    "_template": "/base/main/ArticleMain",

    "datePublished": "2016-02-19T16:43:22+00:00",
    "dateModified": "2016-02-19T17:44:23+00:00",

    "breadcrumbs": {
        "_template": "/base/Breadcrumbs",
        "items": [
            "{{words([1, 3])}}",
            "{{words([1, 3])}}",
            "{{words([1, 3])}}"
        ]
    },

    "slug": "{{words([4,10])}}",
    "headline": "{{words([10,15])}}",
    "subHeadline": "{{words}}",

    "byline": {
        "_template": "/base/util/Concatenated",
        "delimiter": ", ",
        "items": [
            "{{name}}",
            "{{name}}",
            "{{name}}"
        ]
    },

    "lead": {
        "_template": "/base/util/Image",
        "src": "{{image(900,450)}}"
    },

    "body": {
        "_template": "/base/util/Concatenated",
        "items": [
            {
                "_template": "/base/util/RawHtml",
                "html": "{{paragraphs([3,10])}}"
            },
            {
                "_template": "/base/Enhancement",
                "alignment": "left",
                "content": {
                    "_template": "/base/util/Image",
                    "src": "{{image(260,360)}}"
                }
            },
            {
                "_template": "/base/util/RawHtml",
                "html": "<h3>Subheadline</h3>"
            },
            {
                "_template": "/base/util/RawHtml",
                "html": "{{paragraphs([3,10])}}"
            },
            {
                "_template": "/base/Enhancement",
                "alignment": "right",
                "content": {
                    "_template": "/base/util/Image",
                    "src": "{{image(260,360)}}"
                }
            },
            {
                "_template": "/base/util/RawHtml",
                "html": "{{paragraphs([3,10])}}"
            },
            {
                "_template": "/base/util/RawHtml",
                "html": "{{paragraphs([3,10])}}"
            }
        ]
    },

    "tags": "",
    "socialSharing": "",
    "comments": "",

    "relatedContent": {
        "_template": "/base/util/Concatenated",
        "items": [
            {
                "_template": "/base/promo/ListPromo",
                "title": "Related Content",
                "items": [
                    {
                        "_repeat": 4,
                        "_template": "/base/promo/ListPromoItem",
                        "image": {
                            "_template": "/base/util/Image",
                            "src": "{{image(40,40)}}"
                        },
                        "content": "{{words([8,12])}}"
                    }
                ]
            },
            {
                "_template": "/base/promo/ListPromo",
                "title": "Promoted Content",
                "items": [
                    {
                        "_repeat": 2,
                        "_template": "/base/promo/ListPromoItem",
                        "image": {
                            "_template": "/base/util/Image",
                            "src": "{{image(40,40)}}"
                        },
                        "content": "{{words([8,12])}}"
                    }
                ]
            }
        ]
    }
