{

    "_template": "/assets/templates/base/components/gallery-full",

    "displayOptions" : {
        "modifierClass" : "component-inverse-color ratio-16x9",
        "thumbnails" : true
    },

    "heading": {
        "_template" : "/assets/templates/base/common/text",
        "text" : "{{words(10)}}"
    },

    "description" : {
        "_template" : "/assets/templates/base/common/text",
        "text" : "{{words(20)}}"
    },

    "slides": [
        {
            "_repeat" : 2,
            "_template" : "/assets/templates/base/components/gallery-full-slide-content",
            "image" : {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(1600, 900)}}"
                }
            },
            "thumbnail": {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(100, 100)}}"
                }
            },
            "caption" : [
                {
                    "_template": "/assets/templates/base/common/text",
                    "text" : "{{words(20)}}"
                }
            ]
        },
        {
            "_template" : "/assets/templates/base/components/gallery-full-slide-content",
            "image" : {
                "_template": "/assets/templates/base/common/image",
                "cssClass" : "image-portait",
                "extraAttributes": {
                    
                    "src": "{{image(500, 900)}}"
                }
            },
            "thumbnail": {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(100, 100)}}"
                }
            },
            "caption" : [
                {
                    "_template": "/assets/templates/base/common/text",
                    "text" : "{{words(20)}}"
                }
            ]
        },
        {
            "_repeat" : 2,
            "_template" : "/assets/templates/base/components/gallery-full-slide-content",
            "image" : {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(1600, 900)}}"
                }
            },
            "thumbnail": {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(100, 100)}}"
                }
            },
            "caption" : [
                {
                    "_template": "/assets/templates/base/common/text",
                    "text" : "{{words(20)}}"
                }
            ]
        },
        {
            "_template" : "/assets/templates/base/components/gallery-full-interstitial",
            "displayOptions" : {
                "countDisable" : false,
                "countMessage" : "Advertisement: You may skip in {n} seconds",
                "countDuration" : 5,
                "countPosition" : "topRight",
                "navEnabled" : false,
                "navVisible" : true,
                "modifierClass" : "interstitial"
            },
            "contentUrl" : "_interstitial.html"
        },
        {
            "_repeat" : 5,
            "_template" : "/assets/templates/base/components/gallery-full-slide-content",
            "image" : {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(1600, 900)}}"
                }
            },
            "thumbnail": {
                "_template": "/assets/templates/base/common/image",
                "extraAttributes": {
                    
                    "src": "{{image(100, 100)}}"
                }
            },
            "caption" : [
                {
                    "_template": "/assets/templates/base/common/text",
                    "text" : "{{words(20)}}"
                }
            ]
        }
    ],

    "tags" : {
        "_dataUrl" : "/elements/tags/tag-list.json"
    },

    "sharing" : {
        "_dataUrl" : "/elements/social-share/share-monochrome.json"
    }
}
