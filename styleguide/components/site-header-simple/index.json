{
  "_wrapper": "/common/_layout-embed.json",
  "_template": "/assets/templates/base/components/site-header-simple",
  "siteName": "Brightspot Base Styleguide",
  "siteLogo": {
    "_template": "/assets/templates/base/common/link",
    "attributes": {
      "href": "/",
      "title": "Brightspot Base Styleguide",
      "_template": "/assets/templates/base/common/attributes"
    },
    "body": {
      "_template": "/assets/templates/base/common/image",
      "attributes": {
        "alt": "Brightspot",
        "src": "/assets/logo-site.png",
        "_template": "/assets/templates/base/common/attributes"
      }
    }
  },
  "navLinks": [
    {
    "_repeat" : 5,
      "_template": "/assets/templates/base/common/link",
      "attributes": {
        "href": "link-me",
        "title": "I are a link",
        "_template": "/assets/templates/base/common/attributes"
      },
      "body": {
        "_template": "/assets/templates/base/common/text",
        "text": "Menu Item"
      }
    }
  ],

  "searchForm" : {

        "_template": "/assets/templates/base/common/form",

        "modifierClass": "site-header-simple-search",

        "attributes" : {
            "_template": "/assets/templates/base/common/attributes",
            "action" : "/components/bsp-search-results/search-results-ajax-1.html?q=test"
        },

        "inputs" : [
            {
                "_template" : "/assets/templates/base/common/form-input",

                "modifierClass" : "site-search-text",

                "attributes": {
                    "_template": "/assets/templates/base/common/attributes",
                    "autocomplete": "off",
                    "id" : "q",
                    "name" : "q",
                    "required": true,
                    "type": "text"
                }
            },
            {
                "_template" : "/assets/templates/base/common/form-input",

                "modifierClass" : "site-search-submit",

                "isButton" : true,

                "attributes": {
                    "_template": "/assets/templates/base/common/attributes",
                    "type": "submit",
                    "value": "Go"
                }
            }
        ]

    }
}
