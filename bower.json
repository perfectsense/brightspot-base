{
  "name": "brightspot-base",
  "version": "3.0.0",
  "homepage": "https://github.com/perfectsense/brightspot-base",
  "dependencies": {
    "bsp-ajax-links": "perfectsense/brightspot-js-ajax-links#0.0.5",
    "bsp-autosubmit": "perfectsense/brightspot-js-autosubmit#^2.0.0",
    "bsp-carousel": "perfectsense/brightspot-js-carousel#^1.3.0",
    "bsp-feature-detect": "perfectsense/brightspot-js-feature-detect#^1.0.0",
    "bsp-infinite-content": "perfectsense/brightspot-js-infinite-content#0.0.6",
    "bsp-lazyimage": "perfectsense/brightspot-js-lazyimage#^1.0.1",
    "bsp-modal": "perfectsense/brightspot-js-modal#^1.0.0",
    "bsp-share": "perfectsense/brightspot-js-share#^3.0.2",
    "bsp-tabber": "perfectsense/brightspot-js-tabber#^2.0.3",
    "bsp-templates": "perfectsense/brightspot-js-templates#^2.1.3",
    "bsp-toggle-item": "perfectsense/brightspot-js-toggle-item#0.0.1",
    "bsp-utils": "perfectsense/brightspot-js-utils#^3.1.0",
    "jquery": "^2.1.4",
    "vex": "2.3.2",
    "normalize-css": "necolas/normalize.css#^3.0.3"
  },
  "main": "",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "brightspot-library-files": [
    "src/main/webapp/assets/scripts/config.js",
    "src/main/webapp/assets/scripts/main.js",
    "src/main/webapp/assets/styles/main.less"
  ],
  "brightspot-copy-overrides": {
    "brightspot-base": [
      {
        "cwd": "src/main/webapp/assets",
        "src": "base/**",
        "dest": "",
        "expand": true
      }
    ]
  }
}
