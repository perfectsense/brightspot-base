<div class="bsp-container">

	{{#with aboveColumns}}
		{{#each components}}
			{{> (lookup . '_template') }}
		{{/each}}
	{{/with}}

	<div class="bsp-row">

		<div class="bsp-column-half">
			{{#with main}}
				{{#each components}}
					{{> (lookup . '_template') }}
				{{/each}}
			{{/with}}
		</div>

		<div class="bsp-column-half">
			{{#with aside}}
				{{#each components}}
					{{> (lookup . '_template') }}
				{{/each}}
			{{/with}}
		</div>
		
	</div>
</div>