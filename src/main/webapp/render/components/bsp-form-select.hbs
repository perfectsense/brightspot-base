{{#if showMessagesAbove}}
	{{> components/bsp-form-message this}}
{{/if}}
{{#if options}}
	<div class="bsp-form-select">
		{{#if label}}
			<label {{labelAttributes}}>
			{{#if labelAfterInput}} {{else}}{{render label}}{{/if}}
		{{/if}}
			<select {{render attributes}} {{#if autoSubmit}}data-bsp-autosubmit{{/if}}>
				{{#each options}}{{render this}}{{/each}}
			</select>
		{{#if label}}
			{{#if labelAfterInput}}{{render label}}{{else}} {{/if}}
			</label>
		{{/if}}
	</div>
{{/if}}
{{#if showMessagesBelow}}
	{{> components/bsp-form-message this}}
{{/if}}