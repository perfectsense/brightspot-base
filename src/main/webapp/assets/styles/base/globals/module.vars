/**
 * We can define module bottom padding on a global level to have consistency
 */
@space-module-outside: @space-quad;

/**
 * These are the colors for the "inverse" and "secondary" style
 */
@color-module-inverse-background: @color-gray-dark;
@color-module-inverse-text: @color-white;

@color-module-secondary-background: @color-off-white;
@color-module-secondary-text: @color-text;
