<div class="grid-container{{#if modifierClass}} {{modifierClass}}{{/if}}">

    {{#each aboveColumns}}
        {{render this}}
    {{/each}}

	<div class="row">

		<div class="column-third">
            {{#each first}}
                {{render this}}
            {{/each}}
		</div>

		<div class="column-two-thirds">
            {{#each second}}
                {{render this}}
            {{/each}}
		</div>

	</div>
</div>
