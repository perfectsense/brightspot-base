<div class="SocialFeed"{{extraAttributes}}">
    {{#with title}}
        <h2 class="SocialFeed-title">{{this}}</h2>
    {{/with}}

    <ul class="SocialFeed-content">
        {{#each items}}
            <li class="SocialFeed-content-item">
                {{this}}
            </li>
        {{/each}}
    </ul>

    {{#with cta}}
        <div class="SocialFeed-cta">{{this}}</div>
    {{/with}}
</div>